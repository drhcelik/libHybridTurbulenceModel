FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];


internalField   uniform 0.005;

boundaryField
{
    inlet
    {
        type            inletOutlet;
        inletValue      uniform 0.005;
        value           uniform 0.005;
    }
    outlet
    {
        type            zeroGradient;
    }
    side1
    {
        type            zeroGradient;
    }
    side2
    {
        type            zeroGradient;
    }
    lowerUpper1
    {
        type            zeroGradient;
    }
    lowerUpper2
    {
        type            zeroGradient;
    }
    wall
    {
        type            kqRWallFunction;
        value           uniform 0.005;
    }
}


